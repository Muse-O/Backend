{
  "name": "backend",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "sync-model": "node ./modules/sync-sequelize-model.js",
    "dev": "nodemon app start",
    "prettify": "prettier --write *.js **/*.js",
    "coverage": "jest --coverage",

    "test:init": "NODE_ENV=test sequelize db:create --config=./config/config.js && NODE_ENV=test sequelize db:migrate --config=./config/config.js --model-path=./model",
    "window:test:init": "set sequelize db:create --env test --config=./config/config.js && sequelize db:migrate --env test --config=./config/config.js",
    "new:init": "sequelize db:create --env test --config=./config/config.js && sequelize db:migrate --env test --config=./config/config.js --migrations-path ./models",
    "sync-test":"set NODE_ENV=test node ./modules/sync-sequelize-model-testsync.js",

    "test": "NODE_ENV=test jest --forceExit",
    "test:silent": "NODE_ENV=test jest --silent --forceExit",
    "test:coverage": "NODE_ENV=test jest --coverage --forceExit",
    "test:unit": "NODE_ENV=test jest test/unit --forceExit",
    "test:integration": "NODE_ENV=test jest test/integration --forceExit",

    "window:test": "set NODE_ENV=test && jest --forceExit --detectOpenHandles",
    "window:test:silent": "set NODE_ENV=test && jest --silent --forceExit",
    "window:test:coverage": "set NODE_ENV=test && jest --coverage --forceExit --detectOpenHandles",
    "window:test:unit": "set NODE_ENV=test && jest test/unit --forceExit",
    "window:test:integration": "set NODE_ENV=test && jest test/integration --forceExit"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
